{
  "log": "/tmp/door_monitor.log",
  "gpio_pin": 3,
  "gpio_pull_up_down": "up",
  "gpio_monitor_frequency": 2,
  "mqtt": {
    "client_id": "pytest_gpio_monitor",
    "topics": {
      "home-assistant/pytest/gpio_monitor/state": {
        "pubsub": "publish",
        "payload": {
          "state": "Open|Closed"
        }
      }
    }
  },
  "db": {
    "name": "test_door",
    "columns": [
      {
        "col_name": "timestamp",
        "col_type": "integer",
        "col_key": "PRIMARY KEY"
      },
      {
        "col_name": "state",
        "col_type": "text",
        "col_key": "NOT NULL"
      }
    ]
  }
}