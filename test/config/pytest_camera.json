{
  "log": "/tmp/door_camera.log",
  "settings": {
    "iso": {
      "day": 200,
      "night": 800
    },
    "delay": 0,
    "resolution": [3280, 2464],
    "rotation": 180,
    "brightness": 50,
    "contrast": 0,
    "city": "Seattle"
  },
  "capture_path": "/tmp/capture_test.jpg",
  "mqtt": {
    "client_id": "test_camera",
    "topics": {
      "home-assistant/pytest/gpio_monitor/state": {
        "pubsub": "subscribe",
        "payload": {
          "state": "Open"
        }
      },
      "home-assistant/pytest/camera": {
        "pubsub": "subscribe",
        "payload": {
          "capture": true,
          "delay": 0
        }
      }
    }
  },
  "db": {
    "name": "test_door",
    "columns": [
      {
        "col_name": "timestamp",
        "col_type": "integer",
        "col_key": "PRIMARY KEY"
      },
      {
        "col_name": "state",
        "col_type": "text",
        "col_key": "NOT NULL"
      }
    ]
  }
}