{
  "log": "/tmp/door_camera.log",
  "settings": {
    "iso": {
      "day": 200,
      "night": 800
    },
    "delay": 0,
    "resolution": [
      3280,
      2464
    ],
    "rotation": 180,
    "brightness": 50,
    "contrast": 0,
    "city": "Seattle"
  },
  "capture_path": "/tmp/capture_test.jpg",
  "mqtt": {
    "client_id": "test_camera",
    "table_name": "test_door",
    "topics": {
      "hass/pytest/gpio/state": {
        "pubsub": "subscribe",
        "payload": {
          "state": "Open",
          "convo_id": ""
        }
      },
      "hass/pytest/camera": {
        "pubsub": "subscribe",
        "payload": {
          "capture": "CAPTURE",
          "delay": 0
        }
      },
      "hass/pytest/camera/publish": {
        "pubsub": "publish",
        "payload": {
          "state": "publish",
          "convo_id": "",
          "force": "True|False"
        }
      }
    }
  },
  "db": {
    "type": "local",
    "path": "/tmp/some_db.sqlite3",
    "database": "test_door",
    "tables": [
      {
        "name": "test_door",
        "columns": [
          {
            "col_name": "timestamp",
            "col_type": "integer",
            "col_key": "PRIMARY KEY"
          },
          {
            "col_name": "state",
            "col_type": "text",
            "col_key": "NOT NULL"
          },
          {
            "col_name": "convo_id",
            "col_type": "text",
            "col_key": "NOT NULL"
          },
          {
            "col_name": "captured",
            "col_type": "integer",
            "col_key": "NOT NULL"
          },
          {
            "col_name": "notified",
            "col_type": "integer",
            "col_key": "NOT NULL"
          }
        ]
      }
    ]
  }
}