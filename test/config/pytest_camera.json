{
  "log": "/tmp/door_camera.log",
  "settings": {
    "iso": {
      "day": 200,
      "night": 800
    },
    "delay": 0,
    "resolution": [3280, 2464],
    "rotation": 180,
    "brightness": 50,
    "contrast": 0,
    "city": "Seattle"
  },
  "capture_path": "/tmp/capture_test.jpg",
  "mqtt": {
    "client_id": "test_camera",
    "topics": {
      "home-assistant/test/state": {
        "pubsub": "subscribe",
        "payload": {
          "state": "Open"
        }
      },
      "home-assistant/test/camera": {
        "pubsub": "subscribe",
        "payload": {
          "capture": true,
          "delay": 0
        }
      }
    }
  }
}