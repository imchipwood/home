{
  "log": "/home/cpw/dev/logs/garageDoorMonitor.log",
  "gpio_pin": 4,
  "gpio_pull_up_down": "up",
  "mqtt": {
    "client_id": "garage_door_monitor",
    "topics": {
      "home-assistant/garage/state": {
        "pubsub": "publish",
        "payload": {
          "state": "Open|Closed"
        }
      }
    }
  },
  "db": {
    "name": "garage_door_monitor",
    "columns": [
      {
        "col_name": "timestamp",
        "col_type": "integer",
        "col_key": "PRIMARY KEY"
      },
      {
        "col_name": "state",
        "col_type": "text",
        "col_key": "NOT NULL"
      }
    ]
  }
}