  "gpio_toggle_delay": 0.3,
  "gpio_active_direction": "HIGH",
  "mqtt": {
    "client_id": "garage_door_control",
    "topics": {
      "hass/door/garage/control": {
        "pubsub": "subscribe",
        "payload": {
