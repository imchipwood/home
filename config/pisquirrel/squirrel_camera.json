{
  "log": "/home/cpw/dev/logs/squirrelcam.log",
  "settings": {
    "iso": {
      "day": 200,
      "night": 800
    },
    "delay": 0.0,
    "resolution": [
      3280,
      2464
    ],
    "rotation": 180,
    "brightness": 50,
    "contrast": 0,
    "city": "Seattle"
  },
  "capture_path": "/home/cpw/camera/squirrel/{}.jpg",
  "mqtt": {
    "client_id": "squirrel_camera",
    "topics": {
      "home-assistant/squirrel/camera": {
        "pubsub": "subscribe",
        "payload": {
          "state": "OPEN",
          "delay": 0
        }
      }
    }
  },
  "db": {
    "name": "squirrel_monitor",
    "columns": [
      {
        "col_name": "timestamp",
        "col_type": "integer",
        "col_key": "PRIMARY KEY"
      },
      {
        "col_name": "state",
        "col_type": "text",
        "col_key": "NOT NULL"
      },
      {
        "col_name": "captured",
        "col_type": "integer",
        "col_key": "NOT NULL"
      },
      {
        "col_name": "notified",
        "col_type": "integer",
        "col_key": "NOT NULL"
      }
    ]
  }
}