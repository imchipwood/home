{
  "log": "/home/cpw/dev/logs/gpioMonitor.log",
  "gpio_pin": 27,
  "gpio_pull_up_down": "down",
  "gpio_monitor_frequency": 2,
  "mqtt": {
    "client_id": "pi_test_door_monitor",
    "topics": {
      "hass/test/door/state": {
        "pubsub": "publish",
        "payload": {
          "state": "Open|Closed",
          "convo_id": ""
        }
      }
    }
  },
  "db": {
    "name": "test_door",
    "columns": [
      {
        "col_name": "timestamp",
        "col_type": "integer",
        "col_key": "PRIMARY KEY"
      },
      {
        "col_name": "state",
        "col_type": "text",
        "col_key": "NOT NULL"
      },
      {
        "col_name": "convo_id",
        "col_type": "text",
        "col_key": "NOT NULL"
      },
      {
        "col_name": "captured",
        "col_type": "integer",
        "col_key": "NOT NULL"
      },
      {
        "col_name": "notified",
        "col_type": "integer",
        "col_key": "NOT NULL"
      }
    ]
  }
}