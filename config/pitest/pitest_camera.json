{
  "log": "/home/cpw/dev/logs/testDoorCamera.log",
  "settings": {
    "iso": {
      "day": 200,
      "night": 800
    },
    "delay": 0.5,
    "resolution": [3280, 2464],
    "rotation": 180,
    "brightness": 50,
    "contrast": 0,
    "city": "Seattle"
  },
  "capture_path": "/home/cpw/camera/captures/test.jpg",
  "mqtt": {
    "client_id": "pi_test_door_camera",
    "topics": {
      "home-assistant/test/door/state": {
        "pubsub": "subscribe",
        "payload": {
          "state": "Open"
        }
      },
      "home-assistant/test/door/camera": {
        "pubsub": "subscribe",
        "payload": {
          "capture": "CAPTURE"
        }
      },
      "home-assistant/test/door/camera_publish": {
        "pubsub": "publish",
        "payload": {
          "state": "publish"
        }
      }
    }
  },
  "db": {
    "name": "test_door",
    "columns": [
      {
        "col_name": "timestamp",
        "col_type": "integer",
        "col_key": "PRIMARY KEY"
      },
      {
        "col_name": "state",
        "col_type": "text",
        "col_key": "NOT NULL"
      }
    ]
  }
}